{
  "private": true,
  "name": "packemon-root",
  "scripts": {
    "docs": "yarn moon run website:start",
    "moon": "$(yarn bin moon)",
    "packemon": "node ./packages/packemon/cjs/bin.cjs",
    "prerelease": "yarn run setup && yarn moon check --all",
    "release": "yarn prerelease && lerna-release",
    "setup": "node ./scripts/buildPackages.mjs",
    "version": "yarn install && git add yarn.lock"
  },
  "workspaces": [
    "packages/*",
    "scenarios/*",
    "website"
  ],
  "engines": {
    "node": ">=16.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@moonrepo/cli": "^0.20.2",
    "@moonrepo/dev": "^1.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.2.3",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^16.18.4",
    "@types/react": "^17.0.52",
    "@types/semver": "^7.3.13",
    "babel-preset-moon": "^1.1.3",
    "chokidar": "^3.5.3",
    "electron-to-chromium": "^1.4.284",
    "eslint": "^8.28.0",
    "eslint-config-moon": "^1.4.1",
    "jest": "^29.3.1",
    "jest-preset-moon": "^1.1.3",
    "lerna": "^6.0.3",
    "prettier": "^2.8.0",
    "prettier-config-moon": "^1.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-moon": "^1.2.1",
    "typescript": "^4.9.3"
  },
  "funding": {
