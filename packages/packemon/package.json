{
  "name": "packemon",
  "version": "2.0.0",
  "type": "commonjs",
  "description": "Build and prepare packages for npm distribution using standardized configurations and practices. Gotta pack 'em all!",
  "keywords": [
    "build",
    "prepare",
    "pack",
    "package"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/packemon.git",
    "directory": "packages/packemon"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "main": "./cjs/index.cjs",
  "types": "./dts/index.d.ts",
  "bin": "./cjs/bin.cjs",
  "files": [
    "cjs/**/*.{cjs,mjs,map}",
    "dts/**/*.d.ts",
    "src/**/*.{ts,tsx,json}",
    "templates/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./dts/*.d.ts",
      "node": {
        "require": "./cjs/*.cjs"
      }
    },
    ".": {
      "types": "./dts/index.d.ts",
      "node": {
        "import": "./cjs/index-wrapper.mjs",
        "require": "./cjs/index.cjs"
      }
    }
  },
  "engines": {
    "node": ">=14.15.0",
    "npm": ">=6.14.0"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "babel": "src/babel.ts",
      "bin": "src/bin.ts"
    },
    "format": "cjs",
    "platform": "node"
  },
  "dependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@boost/cli": "^3.0.3",
    "@boost/common": "^3.2.1",
    "@boost/config": "^3.2.0",
    "@boost/debug": "^3.0.3",
    "@boost/event": "^3.0.1",
    "@boost/pipeline": "^3.2.2",
    "@boost/terminal": "^3.0.0",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@swc/core": "^1.2.168",
    "babel-plugin-cjs-esm-interop": "^2.0.0",
    "babel-plugin-conditional-invariant": "^2.0.0",
    "babel-plugin-env-constants": "^2.0.0",
    "execa": "^5.1.1",
    "fast-glob": "^3.2.11",
    "filesize": "^8.0.7",
    "fs-extra": "^10.1.0",
    "ink": "^3.2.0",
    "ink-progress-bar": "^3.0.0",
    "ink-spinner": "^4.0.3",
    "magic-string": "^0.26.1",
    "micromatch": "^4.0.5",
    "npm-packlist": "^5.0.0",
    "react": "^17.0.2",
    "resolve": "^1.22.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-node-externals": "^4.0.0",
    "rollup-plugin-polyfill-node": "^0.9.0",
    "semver": "^7.3.7",
    "spdx-license-list": "^6.5.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@types/acorn": "^4.0.6",
    "@typescript-eslint/types": "^5.20.0"
  },
  "peerDependencies": {
    "chokidar": "^3.5.1",
    "typescript": "^4.2.4"
  },
  "peerDependenciesMeta": {
    "chokidar": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  }
}
