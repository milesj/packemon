{
  "name": "packemon",
  "version": "1.9.0",
  "description": "Build and prepare packages for NPM distribution using standardized configurations and practices. Gotta pack 'em all!",
  "keywords": [
    "build",
    "prepare",
    "pack",
    "package"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/packemon.git",
    "directory": "packages/packemon"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "main": "./lib/index.js",
  "types": "./dts/index.d.ts",
  "bin": "./lib/bin.js",
  "files": [
    "babel.js",
    "dts/**/*.d.ts",
    "lib/**/*.{js,map}",
    "src/**/*.{ts,tsx,json}",
    "templates/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "node": "./lib/*.js",
      "types": "./dts/*.d.ts"
    },
    ".": {
      "node": "./lib/index.js",
      "types": "./dts/index.d.ts"
    }
  },
  "engines": {
    "node": ">=12.17.0",
    "npm": ">=6.13.0"
  },
  "packemon": {
    "inputs": {
      "index": "src/index.ts",
      "babel": "src/babel.ts",
      "bin": "src/bin.ts"
    },
    "format": "lib",
    "platform": "node"
  },
  "dependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-proposal-decorators": "^7.16.5",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-flow": "^7.16.5",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@boost/cli": "^3.0.2",
    "@boost/common": "^3.2.0",
    "@boost/debug": "^3.0.2",
    "@boost/event": "^3.0.0",
    "@boost/pipeline": "^3.2.1",
    "@boost/terminal": "^3.0.0",
    "@microsoft/api-extractor": "^7.18.20",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "babel-plugin-cjs-esm-interop": "^1.2.0",
    "babel-plugin-conditional-invariant": "^1.1.0",
    "babel-plugin-env-constants": "^1.1.0",
    "babel-plugin-transform-async-to-promises": "^0.8.16",
    "builtin-modules": "^3.2.0",
    "execa": "^5.1.1",
    "fast-glob": "^3.2.7",
    "filesize": "^8.0.6",
    "fs-extra": "^10.0.0",
    "ink": "^3.2.0",
    "ink-progress-bar": "^3.0.0",
    "ink-spinner": "^4.0.3",
    "micromatch": "^4.0.4",
    "npm-packlist": "^3.0.0",
    "react": "^17.0.2",
    "resolve": "^1.20.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-polyfill-node": "^0.8.0",
    "rollup-plugin-visualizer": "^5.5.2",
    "semver": "^7.3.5",
    "spdx-license-list": "^6.4.0"
  },
  "peerDependencies": {
    "chokidar": "^3.5.1",
    "typescript": "^4.2.4"
  },
  "peerDependenciesMeta": {
    "chokidar": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  }
}
