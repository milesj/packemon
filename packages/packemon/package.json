{
  "name": "packemon",
  "version": "3.1.0",
  "type": "commonjs",
  "description": "Build and prepare packages for npm distribution using standardized configurations and practices. Gotta pack 'em all!",
  "keywords": [
    "build",
    "prepare",
    "pack",
    "package"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:milesj/packemon.git",
    "directory": "packages/packemon"
  },
  "author": "Miles Johnson",
  "license": "MIT",
  "main": "./cjs/index.cjs",
  "types": "./cjs/index.d.ts",
  "bin": "./cjs/bin.cjs",
  "files": [
    "cjs/**/*",
    "src/**/*",
    "templates/**/*"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./babel": {
      "node": {
        "types": "./cjs/babel.d.ts",
        "import": "./cjs/babel-wrapper.mjs",
        "require": "./cjs/babel.cjs"
      }
    },
    "./bin": {
      "node": {
        "types": "./cjs/bin.d.ts",
        "import": "./cjs/bin-wrapper.mjs",
        "require": "./cjs/bin.cjs"
      }
    },
    ".": {
      "node": {
        "types": "./cjs/index.d.ts",
        "import": "./cjs/index-wrapper.mjs",
        "require": "./cjs/index.cjs"
      }
    }
  },
  "engines": {
    "node": ">=16.12.0"
  },
  "packemon": {
    "api": "private",
    "inputs": {
      "index": "src/index.ts",
      "babel": "src/babel.ts",
      "bin": "src/bin.ts"
    },
    "format": "cjs",
    "platform": "node"
  },
  "dependencies": {
    "@babel/core": "^7.22.1",
    "@babel/plugin-proposal-decorators": "^7.22.3",
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-flow": "^7.21.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@boost/cli": "^4.0.1",
    "@boost/common": "^4.1.0",
    "@boost/config": "^4.0.1",
    "@boost/debug": "^4.0.1",
    "@boost/event": "^4.0.1",
    "@boost/pipeline": "^4.0.1",
    "@boost/terminal": "^4.0.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@swc/core": "^1.3.57",
    "babel-plugin-cjs-esm-interop": "^3.0.1",
    "babel-plugin-conditional-invariant": "^3.0.1",
    "babel-plugin-env-constants": "^3.0.1",
    "babel-preset-solid": "^1.7.4",
    "browserslist": "^4.21.7",
    "debug": "^4.3.4",
    "execa": "^5.1.1",
    "fast-glob": "^3.2.12",
    "filesize": "^10.0.7",
    "fs-extra": "^11.1.1",
    "ink": "^3.2.0",
    "ink-progress-bar": "^3.0.0",
    "ink-spinner": "^4.0.3",
    "magic-string": "^0.30.0",
    "micromatch": "^4.0.5",
    "react": "^17.0.2",
    "resolve": "^1.22.2",
    "rollup": "^3.23.0",
    "rollup-plugin-node-externals": "^5.1.3",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "semver": "^7.5.0",
    "spdx-license-list": "^6.6.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@typescript-eslint/types": "^5.59.5"
  },
  "peerDependencies": {
    "chokidar": "^3.5.1",
    "typescript": "^4.2.4 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "chokidar": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/milesjohnson"
  }
}
